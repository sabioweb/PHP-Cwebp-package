{
    "name": "sabioweb/php-cwebp-package",
    "description": "A pure PHP package for converting images to WebP format",
    "type": "library",
    "keywords": ["webp", "image", "converter", "php", "gd"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sabioweb",
            "email": "info@sabioweb.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sabioweb\\WebPConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sabioweb\\WebPConverter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

